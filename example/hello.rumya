let messageGen name = "Hello, " + name + "! ".
nameList := [
    { name: "world" },
    { name: "Rumya", repeat: 3 }
].

let index: num = 0.
while let elm = nameList 's index loop
begin
    if elm =: { name, repeat } then
        print messageGen name * repeat
    else if elm =: { name } then
        print name |> messageGen.
    index += 1
end.
