fun messageGen name = "Hello, " + name + "! ".
let nameList = ["world", ["Rumya", 3]].
let index: number = 0.

while let item = nameList[index] loop
begin
    if let [name, count] = item then
    begin
        greeting := messageGen name.
        print greeting * count
    end
    else
        print messageGen item.
    index += 1
end.
