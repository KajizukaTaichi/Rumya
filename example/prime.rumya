TRUE as num := 1.
MAX as num := 100.

let isPrime n =
begin
    n > 1.
    if n < 4 then
        TRUE
    else
        for count in
        2 ~ (n ^ 0.5 + 1) do
            n % count != 0.
    TRUE
end.

let count = 0.
for i in 0 ~ MAX do
    if isPrime i then
    begin
        print i.
        count += 1
    end.
print "discovered ", count
